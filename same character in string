class Solution 
{ 
    int sameChar(String a, String b) 
    {
        // code here
        int count=0;
        a=a.toLowerCase();
        b=b.toLowerCase();
        char[] a1=a.toCharArray();
        char[] b1=b.toCharArray();
        for(int i=0;i<a.length()||i<b.length();i++){
            if(a1[i]==b1[i])
                count++;
        }
        return count;
    }
}
